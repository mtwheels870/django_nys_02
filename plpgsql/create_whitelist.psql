CREATE OR REPLACE PROCEDURE create_whitelist(
    IN survey_id INT,
    OUT whitelist_tablename VARCHAR(50)
) 
LANGUAGE plpgsql
AS $$
<< outerblock >>
DECLARE
    select_statement VARCHAR(400);
BEGIN
    whitelist_tablename := 'whitelist_' || survey_id;

    RAISE NOTICE 'whitelist_tablename: %', whitelist_tablename;
END;
$$;
