CREATE OR REPLACE PROCEDURE create_whitelist(
    IN p_survey_id INT
) 
LANGUAGE plpgsql
AS $$
BEGIN
    p_whitelist_tablename := 'whitelist_' || p_survey_id;

    UPDATE powerscan_iprangesurvey
    SET whitelist_tablename = p_whitelist_tablename
    WHERE id = p_survey_id;

    COMMIT;
    RAISE LOG '1. after update';
END
$$;
