CREATE OR REPLACE PROCEDURE create_whitelist(
    IN p_survey_id INT,
    OUT p_whitelist_tablename VARCHAR(50)
) 
LANGUAGE plpgsql
AS $$
<< outerblock >>
DECLARE
    select_statement VARCHAR(400);
    update_statement VARCHAR(150);
BEGIN
    p_whitelist_tablename := 'whitelist_' || survey_id;

    UPDATE powerscan_iprangesurvey
    SET whitelist_tablename = p_whitelist_tablename
    WHERE id = p_survey_id;
END;
$$;
